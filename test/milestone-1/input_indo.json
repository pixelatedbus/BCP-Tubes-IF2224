  "start": {
    " ": "start", "\n": "start", "\t": "start",

    "[a-zA-Z]": "id",
    "[0-9]": "num",

    ":": "colon",
    "+": "plus",
    "-": "minus",
    "*": "mul",
    "/": "div",
    ";": "semi",
    "(": "lparen",
    ")": "rparen",
    "[": "lbracket",
    "]": "rbracket",
    "'": "char_unfinished_1",
    "{": "lcurly",
    ",": "comma",
    ".": "dot",
    "=": "equal",
    "<": "less_than",
    ">": "greater_than"
  },

  "dot": {
    ".": "range"
  },

  "id": {
    "_": "id",
    "[a-zA-Z0-9]": "id"
  },

  "less_than": {
    "=": "less_equal",
    ">": "not_equal"
  },

  "greater_than": {
    "=": "greater_equal"
  },

  "num": {
    "[0-9]": "num",
    "E": "num_exponent_unfinished",
    "[a-zA-Z_]": "num_error"
  },

  "num_exponent_unfinished": {
    "[0-9]": "num_exponent",
    "+": "num_exponent_sign",
    "[a-zA-Z_]": "num_error"
  },

  "num_exponent_sign": {
    "[0-9]": "num_exponent",
    "[a-zA-Z_]": "num_error"
  },

  "num_exponent": {
    "[0-9]": "num_exponent",
    "[a-zA-Z_]": "num_error"
  },

  "num_error": {
    "[a-zA-Z0-9_]": "num_error"
  },

  "colon": {
    "=": "assign"
  },

  "string_unfinished": {
    "'": "string", "[a-zA-Z0-9_ !@#$%^&*()+=.,<>;:]": "string_unfinished"
  },

  "string" : {
    "'": "string_unfinished"
  },

  "char_unfinished_1": {
    "'": "char",
    "[a-zA-Z0-9_ !@#$%^&*()+=.,<>;:]": "char_unfinished_2"
  },

  "char_unfinished_2": {
    "'": "char",
    "[a-zA-Z0-9_ !@#$%^&*()+=.,<>;:]": "string_unfinished"
  },

  "lparen": {
    "*": "comment_unfinished_paren"
  },

  "lcurly": {
    "[a-zA-Z0-9_ !@#$%^&*()+=.,<>;:\n\t\\{]": "comment_unfinished_curly",
    "}": "comment_end"
  },

  "comment_unfinished_curly": {
    "[a-zA-Z0-9_ !@#$%^&*()+=.,<>;:\n\t\\{]": "comment_unfinished_curly",
    "}": "comment_end"
  },
  
  "comment_unfinished_paren": {
    "[a-zA-Z0-9_ !@#$%^&()+=.,<>;\\{\\}:\n\t]": "comment_unfinished_paren",
    "*": "comment_half"
  },  

  "comment_half": {
    "[a-zA-Z0-9_ !@#$%^&(+=.,<>;:\\{\\}\n\t]": "comment_unfinished_paren",
    ")": "comment_end",
    "*": "comment_half"
  }
